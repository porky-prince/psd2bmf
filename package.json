{
    "name": "psd2bmf",
    "version": "1.0.0",
    "description": "Convert the PSD to BMF.",
    "author": "Kay",
    "license": "MIT",
    "main": "dist/main.js",
    "bin": {
        "psd2bmf": "bin/psd2bmf.js"
    },
    "files": [
        "bin",
        "dist"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "scripts": {
        "start": "npm run build -- -w -s inline",
        "clean": "del-cli dist",
