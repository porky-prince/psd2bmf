        "exports": "node test/exports.test.js",
        "test": "jest",
        "build": "babel src -d dist --copy-files"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.7",
        "@babel/core": "^7.7.7",
        "@babel/preset-env": "^7.7.7",
        "babel-jest": "^24.9.0",
        "del": "^5.1.0",
        "del-cli": "^3.0.0",
        "jest": "^24.9.0"
    },
    "dependencies": {
        "fs-extra": "^8.1.0",
        "layout": "^2.2.0",
        "psd": "^3.2.0",
        "yargs": "^15.1.0"
    },
    "keywords": [
        "psd",
        "bmf",
        "bmfont",
        "fnt",
        "sprite"
